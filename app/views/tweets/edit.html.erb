<%= form_for @tweet do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name, :size => 140 %> 
    <%= f.submit "編集する" %>

    <%= f.label :category %>
    <%= f.text_field :category, :size => 140 %> 
    <%= f.submit "編集する" %>

    <%= f.label :question %>
    <%= f.text_field :question, :size => 140 %> 
    <%= f.submit "編集する" %>

    <%= f.label :image %>
    <%= f.text_field :image, :size => 140 %> 
    <%= f.submit "編集する" %>

    <%= f.label :user_id %>
